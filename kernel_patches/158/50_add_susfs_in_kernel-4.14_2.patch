+	if (dentry && !IS_ERR(dentry) && dentry->d_inode && susfs_is_inode_sus_path(dentry->d_inode)) {
+		return ERR_PTR(susfs_inode_permission(dir, flags));
+	}
+#endif
 	return dentry;
 }
 
